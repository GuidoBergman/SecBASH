GOOGLE_API_KEY=

# HuggingFace (benchmarks only - for HF Inference models)
# Get one at: https://huggingface.co/settings/tokens
HF_TOKEN=

# OpenRouter (benchmarks only - for phi-4 model)
# Get one at: https://openrouter.ai/
OPENROUTER_API_KEY=
